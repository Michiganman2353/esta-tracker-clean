{
  "name": "firebase",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/firebase/src",
  "projectType": "library",
  "tags": ["type:lib", "scope:firebase"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc",
        "cwd": "packages/firebase"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "packages/firebase"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run --passWithNoTests",
        "cwd": "packages/firebase"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rm -rf dist",
        "cwd": "packages/firebase"
      }
    }
  }
}
